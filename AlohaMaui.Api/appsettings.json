{
  "Logging": {
    "LogLevel": {
      "Default": "Error",
      "Microsoft.AspNetCore": "Error"
    }
  },
  "AllowedHosts": "https://helphawaii.org;https://aloha-maui-api.azurewebsites.net",
  "Test": {
    "Message": "dev"
  },
  "CosmosDb": {
    "Account": "${{ secrets.COSMOSDB_ACCOUNT }}",
    "Key": "${{ secrets.COSMOSDB_KEY }}",
    "Database": "aloha-maui"
  },
  "Auth": {
    "JwtSecret": "${{ secrets.JWT_SECRET }}",
    "Google": {
      "ClientId": "${{ secrets.AUTH_GOOGLE_CLIENT_ID }}",
      "SecretKey": "${{ secrets.AUTH_GOOGLE_SECRET_KEY  }}"
    }
  },
  "Azure": {
    "Blob": {
      "ConnectionString": "${{ secrets.AZURE_BLOB_CONNECTION_STRING }}"
    }
  }
}
